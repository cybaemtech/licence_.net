{
  "name": "license-procurement-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run backend\" \"npm run frontend\" --names \"PHP,React\" --prefix-colors \"cyan,magenta\"",
    "backend": "php -S 127.0.0.1:8000 -t api",
    "frontend": "vite --host 0.0.0.0 --port 5000",
    "dev": "npm run start",
    "dev:full": "npm run build && npm run preview",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "preview": "concurrently \"npm run backend\" \"vite preview --host 0.0.0.0 --port 5000\" --names \"PHP,Preview\" --prefix-colors \"cyan,green\"",
    "preview:frontend-only": "vite preview --host 0.0.0.0 --port 5000"
  },
  "dependencies": {
    "concurrently": "^9.2.1",
    "date-fns": "^3.3.1",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.22.2",
    "recharts": "^2.12.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "vite": "^5.1.6"
  }
}
